<%- include("../../../../templates/head"); %>
<div class="row">
    <div class="col-lg-12">
        <hr />
        <h2 id="api_create_book">연재본 등록</h2>
        <form method="POST" action="/admin/book/serialization/content" enctype ="multipart/form-data">
            <div class="form-group">
                <label>Title</label>
                <input type="text" class="form-control" name="title" value="" />
            </div>
            <div class="form-group">
                <label>File</label>
                <input type="file" class="form-control" name="file" value="">
            </div>
            <div class="form-group">
                <label>Serialization_book_id</label>
                <input type="text" class="form-control" name="serialization_book_id" value=<%=serialization_book_id%>>
            </div>
            <button type="submit" class="btn btn-primary">Submit</button>
        </form>
    </div>
</div>


<%- include("../../../../templates/foot"); %>
